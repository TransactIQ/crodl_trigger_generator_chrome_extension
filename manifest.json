{
  "manifest_version": 3,
  "name": "TradingIQ Crodl Message Generator",
  "version": "1.0",
  "author": "andreasjpunkt& vgeyer",
  "description": "Generate Alerts for Signals from TradingIQ indicators for usage with the CrodlTrading platform",
  "permissions": ["storage", "clipboardWrite"],
  "action": {
    "default_icon": {
      "16": "images/logo_16.png",
      "32": "images/logo_32.png",
      "48": "images/logo_48.png",
      "64": "images/logo_64.png",
      "128": "images/logo_128.png"
    },
    "default_title": "TIQ CRODL",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "64": "images/logo_64.png",
    "128": "images/logo_128.png"
  }
}